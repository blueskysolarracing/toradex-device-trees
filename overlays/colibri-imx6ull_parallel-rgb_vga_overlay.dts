// Enable the parallel RGB interface on the Colibri iMX6ULL. Make sure to also
// select a valid display configuration.

/dts-v1/;
/plugin/;

/ {
	compatible = "toradex,colibri-imx6ull-emmc";
};

&backlight {
	status = "okay";
};

&lcdif {
	status = "okay";
};

&pwm4 {
	status = "okay";
};
